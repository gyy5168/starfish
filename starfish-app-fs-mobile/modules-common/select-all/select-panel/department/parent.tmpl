<li class='parent-item JS-parent-item <%= className %>' data-id="<%= id %>" data-parent=true>
	<% if ( avatar ) { %>
		<span class="item-avatar" style="background-image:url(<%= avatar %>?width=70&height=70)">
		</span>
	<% } %>
	<div class="item-name"><%= name %></div>
	<span class="checkbox JS-parent-checkbox"></span>
</li>