<li class='permissions-item JS-item'  data-id="<%= id %>" data-name="<%= name %>">
	<img src='<%= avatar %>?width=70&height=70'>
	<span class="name"><%= name %></span>

	<div class="role-select">
		<span class="role-label JS-role">设置权限级别</span>
		<ul class="role-panel JS-role-panel">
			<li data-value=1>所有者</li>
			<li data-value=2>编辑者</li>
			<li data-value=3>查看者</li>
			<li class="last" data-value=0>删除</li>
		</ul>
	</div>
	
</li>